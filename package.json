{
  "name": "quantum-ui",
  "private": false,
  "version": "0.1.0",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/quantum-ui.umd.js",
  "module": "./dist/quantum-ui.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/quantum-ui.es.js"
    },
    "./Button": {
      "types": "./dist/components/Button/index.d.ts",
      "import": "./dist/components/Button/index.es.js"
    },
    "./TextField": {
      "types": "./dist/components/TextField/index.d.ts",
      "import": "./dist/components/TextField/index.es.js"
    },
    "./Paper": {
      "types": "./dist/components/Paper/index.d.ts",
      "import": "./dist/components/Paper/index.es.js"
    },
    "./Typography": {
      "types": "./dist/components/Typography/index.d.ts",
      "import": "./dist/components/Typography/index.es.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc --project tsconfig.lib.json && vite build",
    "build:types": "tsc --project tsconfig.lib.json --emitDeclarationOnly",
    "build:lib": "vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/quantum-ui.es.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@mui/material": "^6.0.0 || ^7.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@eslint/js": "^9.25.0",
    "@mui/material": "^7.1.0",
    "@storybook/react-vite": "^9.0.6",
    "@types/node": "^22.15.30",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^9.0.6",
    "globals": "^16.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "storybook": "^9.0.6",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
